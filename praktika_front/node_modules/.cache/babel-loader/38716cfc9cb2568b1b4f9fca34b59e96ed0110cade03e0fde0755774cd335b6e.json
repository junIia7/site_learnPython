 = \\\"Hello, world!\\\"\\n\",\"print(sentence.upper())  # \\u0412\\u044B\\u0432\\u043E\\u0434: HELLO, WORLD!\\n\",\"print(sentence.lower())  # \\u0412\\u044B\\u0432\\u043E\\u0434: hello, world!\\n\"]})}),/*#__PURE__*/_jsx(\"p\",{children:\"\\u041A\\u0440\\u043E\\u043C\\u0435 \\u044D\\u0442\\u0438\\u0445 \\u043C\\u0435\\u0442\\u043E\\u0434\\u043E\\u0432 \\u0435\\u0441\\u0442\\u044C \\u0441\\u043B\\u0435\\u0434\\u0443\\u044E\\u0449\\u0438\\u0435 \\u043F\\u043E\\u043F\\u0443\\u043B\\u044F\\u0440\\u043D\\u044B\\u0435 \\u043C\\u0435\\u0442\\u043E\\u0434\\u044B:\"}),/*#__PURE__*/_jsxs(\"ul\",{children:[/*#__PURE__*/_jsxs(\"li\",{children:[/*#__PURE__*/_jsx(\"code\",{children:\"strip()\"}),\" - \\u0443\\u0434\\u0430\\u043B\\u044F\\u0435\\u0442 \\u043D\\u0430\\u0447\\u0430\\u043B\\u044C\\u043D\\u044B\\u0435 \\u0438 \\u043A\\u043E\\u043D\\u0435\\u0447\\u043D\\u044B\\u0435 \\u043F\\u0440\\u043E\\u0431\\u0435\\u043B\\u044C\\u043D\\u044B\\u0435 \\u0441\\u0438\\u043C\\u0432\\u043E\\u043B\\u044B \\u0441\\u0442\\u0440\\u043E\\u043A\\u0438.\"]}),/*#__PURE__*/_jsxs(\"li\",{children:[/*#__PURE__*/_jsx(\"code\",{children:\"replace(old, new)\"}),\" - \\u0437\\u0430\\u043C\\u0435\\u043D\\u044F\\u0435\\u0442 \\u0432\\u0441\\u0435 \\u0432\\u0445\\u043E\\u0436\\u0434\\u0435\\u043D\\u0438\\u044F \\u043F\\u043E\\u0434\\u0441\\u0442\\u0440\\u043E\\u043A\\u0438 \",/*#__PURE__*/_jsx(\"code\",{children:\"old\"}),\" \\u043D\\u0430 \",/*#__PURE__*/_jsx(\"code\",{children:\"new\"}),\".\"]}),/*#__PURE__*/_jsxs(\"li\",{children:[/*#__PURE__*/_jsx(\"code\",{children:\"find(sub)\"}),\" - \\u043D\\u0430\\u0445\\u043E\\u0434\\u0438\\u0442 \\u043F\\u0435\\u0440\\u0432\\u043E\\u0435 \\u0432\\u0445\\u043E\\u0436\\u0434\\u0435\\u043D\\u0438\\u0435 \\u043F\\u043E\\u0434\\u0441\\u0442\\u0440\\u043E\\u043A\\u0438 \",/*#__PURE__*/_jsx(\"code\",{children:\"sub\"}),\" \\u0432 \\u0441\\u0442\\u0440\\u043E\\u043A\\u0435 \\u0438 \\u0432\\u043E\\u0437\\u0432\\u0440\\u0430\\u0449\\u0430\\u0435\\u0442 \\u0435\\u0433\\u043E \\u0438\\u043D\\u0434\\u0435\\u043A\\u0441.\"]}),/*#__PURE__*/_jsxs(\"li\",{children:[/*#__PURE__*/_jsx(\"code\",{children:\"count(sub)\"}),\" - \\u0432\\u043E\\u0437\\u0432\\u0440\\u0430\\u0449\\u0430\\u0435\\u0442 \\u043A\\u043E\\u043B\\u0438\\u0447\\u0435\\u0441\\u0442\\u0432\\u043E \\u0432\\u0445\\u043E\\u0436\\u0434\\u0435\\u043D\\u0438\\u0439 \\u043F\\u043E\\u0434\\u0441\\u0442\\u0440\\u043E\\u043A\\u0438 \",/*#__PURE__*/_jsx(\"code\",{children:\"sub\"}),\" \\u0432 \\u0441\\u0442\\u0440\\u043E\\u043A\\u0435.\"]}),/*#__PURE__*/_jsxs(\"li\",{children:[/*#__PURE__*/_jsx(\"code\",{children:\"startswith(prefix)\"}),\" - \\u043F\\u0440\\u043E\\u0432\\u0435\\u0440\\u044F\\u0435\\u0442, \\u043D\\u0430\\u0447\\u0438\\u043D\\u0430\\u0435\\u0442\\u0441\\u044F \\u043B\\u0438 \\u0441\\u0442\\u0440\\u043E\\u043A\\u0430 \\u0441 \\u0437\\u0430\\u0434\\u0430\\u043D\\u043D\\u043E\\u0433\\u043E \\u043F\\u0440\\u0435\\u0444\\u0438\\u043A\\u0441\\u0430 \",/*#__PURE__*/_jsx(\"code\",{children:\"prefix\"}),\".\"]})]})]}),/*#__PURE__*/_jsxs(\"section\",{className:\"lesson-section\",children:[/*#__PURE__*/_jsx(\"h2\",{children:\"\\u0417\\u0430\\u043A\\u043B\\u044E\\u0447\\u0435\\u043D\\u0438\\u0435\"}),/*#__PURE__*/_jsx(\"p\",{children:\"\\u0421\\u0442\\u0440\\u043E\\u043A\\u0438 \\u044F\\u0432\\u043B\\u044F\\u044E\\u0442\\u0441\\u044F \\u0432\\u0430\\u0436\\u043D\\u043E\\u0439 \\u0447\\u0430\\u0441\\u0442\\u044C\\u044E \\u043F\\u0440\\u043E\\u0433\\u0440\\u0430\\u043C\\u043C\\u0438\\u0440\\u043E\\u0432\\u0430\\u043D\\u0438\\u044F \\u043D\\u0430 Python, \\u0438\\u0441\\u043F\\u043E\\u043B\\u044C\\u0437\\u0443\\u0435\\u043C\\u043E\\u0439 \\u0434\\u043B\\u044F \\u0440\\u0430\\u0431\\u043E\\u0442\\u044B \\u0441 \\u0442\\u0435\\u043A\\u0441\\u0442\\u043E\\u0432\\u044B\\u043C\\u0438 \\u0434\\u0430\\u043D\\u043D\\u044B\\u043C\\u0438. \\u0412 \\u044D\\u0442\\u043E\\u043C \\u0443\\u0440\\u043E\\u043A\\u0435 \\u043C\\u044B \\u0440\\u0430\\u0441\\u0441\\u043C\\u043E\\u0442\\u0440\\u0435\\u043B\\u0438 \\u043E\\u043F\\u0435\\u0440\\u0430\\u0446\\u0438\\u0438 \\u043D\\u0430\\u0434 \\u0441\\u0442\\u0440\\u043E\\u043A\\u0430\\u043C\\u0438, \\u0444\\u043E\\u0440\\u043C\\u0430\\u0442\\u0438\\u0440\\u043E\\u0432\\u0430\\u043D\\u0438\\u0435 \\u0441\\u0442\\u0440\\u043E\\u043A \\u0438 \\u043E\\u0441\\u043D\\u043E\\u0432\\u043D\\u044B\\u0435 \\u043C\\u0435\\u0442\\u043E\\u0434\\u044B \\u0440\\u0430\\u0431\\u043E\\u0442\\u044B \\u0441\\u043E \\u0441\\u0442\\u0440\\u043E\\u043A\\u0430\\u043C\\u0438. \\u041F\\u043E\\u043D\\u0438\\u043C\\u0430\\u043D\\u0438\\u0435 \\u044D\\u0442\\u0438\\u0445 \\u043A\\u043E\\u043D\\u0446\\u0435\\u043F\\u0446\\u0438\\u0439 \\u043F\\u043E\\u043C\\u043E\\u0436\\u0435\\u0442 \\u0432\\u0430\\u043C \\u044D\\u0444\\u0444\\u0435\\u043A\\u0442\\u0438\\u0432\\u043D\\u043E \\u043C\\u0430\\u043D\\u0438\\u043F\\u0443\\u043B\\u0438\\u0440\\u043E\\u0432\\u0430\\u0442\\u044C \\u0438 \\u043E\\u0431\\u0440\\u0430\\u0431\\u0430\\u0442\\u044B\\u0432\\u0430\\u0442\\u044C \\u0442\\u0435\\u043A\\u0441\\u0442\\u043E\\u0432\\u044B\\u0435 \\u0434\\u0430\\u043D\\u043D\\u044B\\u0435 \\u0432 \\u0432\\u0430\\u0448\\u0438\\u0445 \\u043F\\u0440\\u043E\\u0433\\u0440\\u0430\\u043C\\u043C\\u0430\\u0445. \\u041F\\u0440\\u0430\\u043A\\u0442\\u0438\\u043A\\u0443\\u0439\\u0442\\u0435\\u0441\\u044C \\u0438 \\u044D\\u043A\\u0441\\u043F\\u0435\\u0440\\u0438\\u043C\\u0435\\u043D\\u0442\\u0438\\u0440\\u0443\\u0439\\u0442\\u0435 \\u0441 \\u0440\\u0430\\u0437\\u043B\\u0438\\u0447\\u043D\\u044B\\u043C\\u0438 \\u043C\\u0435\\u0442\\u043E\\u0434\\u0430\\u043C\\u0438, \\u0447\\u0442\\u043E\\u0431\\u044B \\u043B\\u0443\\u0447\\u0448\\u0435 \\u0443\\u0441\\u0432\\u043E\\u0438\\u0442\\u044C \\u043C\\u0430\\u0442\\u0435\\u0440\\u0438\\u0430\\u043B!\"})]}),/*#__PURE__*/_jsx(ScrollToTopButton,{})]});};export default Strings;","map":{"version":3,"names":["React","ScrollToTopButton","jsx","_jsx","jsxs","_jsxs","Strings","className","children"],"sources":["C:/Users/kasat/Desktop/praktika_front/src/pages/Strings.js"],"sourcesContent":["import React from 'react';\r\nimport ScrollToTopButton from '../components/ScrollTopButton';\r\n\r\nconst Strings = () => {\r\n  return (\r\n    <div className=\"lesson-container\">\r\n      <header className=\"lesson-header\">\r\n        <h1>Строки</h1>\r\n      </header>\r\n\r\n      <section className=\"lesson-section\">\r\n        <h2>Определение</h2>\r\n        <p>Строка в Python представляет собой последовательность символов, заключённых в кавычки. \r\n          Она может содержать буквы, цифры, символы и пробелы. Строки в Python имеют множество \r\n          операций и методов для работы с ними.</p>\r\n      </section>\r\n\r\n      <section className=\"lesson-section\">\r\n        <h2>Операции над строками</h2>\r\n        <p>Строки в Python поддерживают различные операции, такие как конкатенация (слияние строк \r\n          с помощью оператора <code>+</code>), повторение (умножение строки на число), доступ к \r\n          отдельным символам по индексу. Например:</p>\r\n        <pre>\r\n          <code>\r\n            {`str1 = \"Hello\"\\n`}\r\n            {`str2 = \"World\"\\n`}\r\n            {`concatenated = str1 + \" \" + str2\\n`}\r\n            {`print(concatenated)  # Вывод: Hello World\\n\\n`}\r\n\r\n            {`repeated = str1 * 3\\n`}\r\n            {`print(repeated)  # Вывод: HelloHelloHello\\n\\n`}\r\n\r\n            {`first_char = str1[0]\\n`}\r\n            {`print(first_char)  # Вывод: H\\n`}\r\n          </code>\r\n        </pre>\r\n        <p>Операции над строками позволяют объединять, изменять и извлекать информацию из текстовых \r\n          данных в Python. Кроме этих операций, строки в Python поддерживают срезы. Срезы позволяют получать \r\n          подстроку из строки по определённому диапазону индексов. Они используются для извлечения части \r\n          строки без изменения исходной строки. </p>\r\n        <p>Синтаксис срезов имеет вид <code>string[start:stop:step]</code>, где:</p>\r\n        <ul>\r\n          <li><code>start</code> - начальный индекс включительно (по умолчанию 0).</li>\r\n          <li><code>stop</code> - конечный индекс исключительно (по умолчанию до конца строки).</li>\r\n          <li><code>step</code> - шаг среза (по умолчанию 1).</li>\r\n        </ul>\r\n        <p>Примеры использования срезов:</p>\r\n        <pre>\r\n          <code>\r\n            {`str = \"Hello, World!\"\\n\\n`}\r\n\r\n            {`# Извлечение части строки\\n`}\r\n            {`substring = str[7:12]\\n`}\r\n            {`print(substring)  # Вывод: World\\n\\n`}\r\n\r\n            {`# Использование отрицательного шага для \"переворота строки\"\\n`}\r\n            {`# Отрицательные индексы ведут отчсет элементов с единицы, начиная с конца строки\\n`}\r\n            {`# Если шаг - отрицательный, по умолчанию start = конец строки, end = 0\\n`}\r\n            {`reversed_str = str[::-1] \\n`}\r\n            {`print(reversed_str)  # Вывод: !dlroW ,olleH\\n`}\r\n          </code>\r\n        </pre>\r\n      </section>\r\n\r\n      <section className=\"lesson-section\">\r\n        <h2>Форматирование строк</h2>\r\n        <p>Форматирование строк в Python может быть выполнено различными способами, включая \r\n          использование оператора <code>%</code>, метода <code>.format()</code> и f-строк \r\n          (форматированных строк). Эти методы позволяют встраивать значения переменных в строки и \r\n          контролировать их форматирование. Например:</p>\r\n        <pre>\r\n          <code>\r\n            {`name = \"Alice\"\\n`}\r\n            {`age = 30\\n`}\r\n            {`formatted_str = \"Name: %s, Age: %d\" % (name, age)\\n`}\r\n            {`print(formatted_str)  # Вывод: Name: Alice, Age: 30\\n\\n`}\r\n\r\n            {`formatted_str2 = \"Name: {}, Age: {}\".format(name, age)\\n`}\r\n            {`print(formatted_str2)  # Вывод: Name: Alice, Age: 30\\n\\n`}\r\n\r\n            {`formatted_str3 = f\"Name: {name}, Age: {age}\"\\n`}\r\n            {`print(formatted_str3)  # Вывод: Name: Alice, Age: 30\\n`}\r\n          </code>\r\n        </pre>\r\n        <p>Форматирование строк в Python является мощным инструментом для создания читаемого и \r\n          удобного для использования вывода.</p>\r\n      </section>\r\n\r\n      <section className=\"lesson-section\">\r\n        <h2>Методы строк</h2>\r\n        <p>Строки в Python имеют множество встроенных методов, которые облегчают их манипулирование. \r\n          Некоторые из часто используемых методов включают <code>upper()</code> для преобразования в \r\n          верхний регистр, <code>lower()</code> для преобразования в нижний регистр:</p>\r\n        <pre>\r\n          <code>\r\n            {`sentence = \"Hello, world!\"\\n`}\r\n            {`print(sentence.upper())  # Вывод: HELLO, WORLD!\\n`}\r\n            {`print(sentence.lower())  # Вывод: hello, world!\\n`}\r\n          </code>\r\n        </pre>\r\n        <p>Кроме этих методов есть следующие популярные методы:</p>\r\n        <ul>\r\n          <li><code>strip()</code> - удаляет начальные и конечные пробельные символы строки.</li>\r\n          <li><code>replace(old, new)</code> - заменяет все вхождения подстроки <code>old</code> на <code>new</code>.</li>\r\n          <li><code>find(sub)</code> - находит первое вхождение подстроки <code>sub</code> в строке и возвращает его индекс.</li>\r\n          <li><code>count(sub)</code> - возвращает количество вхождений подстроки <code>sub</code> в строке.</li>\r\n          <li><code>startswith(prefix)</code> - проверяет, начинается ли строка с заданного префикса <code>prefix</code>.</li>\r\n        </ul>\r\n      </section>\r\n\r\n      <section className=\"lesson-section\">\r\n        <h2>Заключение</h2>\r\n        <p>Строки являются важной частью программирования на Python, используемой для работы с \r\n          текстовыми данными. В этом уроке мы рассмотрели операции над строками, форматирование \r\n          строк и основные методы работы со строками. Понимание этих концепций поможет вам \r\n          эффективно манипулировать и обрабатывать текстовые данные в ваших программах. \r\n          Практикуйтесь и экспериментируйте с различными методами, чтобы лучше усвоить материал!</p>\r\n      </section>\r\n      <ScrollToTopButton /> \r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Strings;\r\n"],"mappings":"AAAA,MAAO,CAAAA,KAAK,KAAM,OAAO,CACzB,MAAO,CAAAC,iBAAiB,KAAM,+BAA+B,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAE9D,KAAM,CAAAC,OAAO,CAAGA,CAAA,GAAM,CACpB,mBACED,KAAA,QAAKE,SAAS,CAAC,kBAAkB,CAAAC,QAAA,eAC/BL,IAAA,WAAQI,SAAS,CAAC,eAAe,CAAAC,QAAA,cAC/BL,IAAA,OAAAK,QAAA,CAAI,sCAAM,CAAI,CAAC,CACT,CAAC,cAETH,KAAA,YAASE,SAAS,CAAC,gBAAgB,CAAAC,QAAA,eACjCL,IAAA,OAAAK,QAAA,CAAI,oEAAW,CAAI,CAAC,cACpBL,IAAA,MAAAK,QAAA,CAAG,6/BAEoC,CAAG,CAAC,EACpC,CAAC,cAEVH,KAAA,YAASE,SAAS,CAAC,gBAAgB,CAAAC,QAAA,eACjCL,IAAA,OAAAK,QAAA,CAAI,sHAAqB,CAAI,CAAC,cAC9BH,KAAA,MAAAG,QAAA,EAAG,shBACmB,cAAAL,IAAA,SAAAK,QAAA,CAAM,GAAC,CAAM,CAAC,4cACM,EAAG,CAAC,cAC9CL,IAAA,QAAAK,QAAA,cACEH,KAAA,SAAAG,QAAA,kVAWM,CAAC,CACJ,CAAC,cACNL,IAAA,MAAAK,QAAA,CAAG,4lDAGqC,CAAG,CAAC,cAC5CH,KAAA,MAAAG,QAAA,EAAG,gJAA2B,cAAAL,IAAA,SAAAK,QAAA,CAAM,yBAAuB,CAAM,CAAC,wBAAM,EAAG,CAAC,cAC5EH,KAAA,OAAAG,QAAA,eACEH,KAAA,OAAAG,QAAA,eAAIL,IAAA,SAAAK,QAAA,CAAM,OAAK,CAAM,CAAC,mPAAkD,EAAI,CAAC,cAC7EH,KAAA,OAAAG,QAAA,eAAIL,IAAA,SAAAK,QAAA,CAAM,MAAI,CAAM,CAAC,kUAAgE,EAAI,CAAC,cAC1FH,KAAA,OAAAG,QAAA,eAAIL,IAAA,SAAAK,QAAA,CAAM,MAAI,CAAM,CAAC,gIAA8B,EAAI,CAAC,EACtD,CAAC,cACLL,IAAA,MAAAK,QAAA,CAAG,iKAA6B,CAAG,CAAC,cACpCL,IAAA,QAAAK,QAAA,cACEH,KAAA,SAAAG,QAAA,s2CAYM,CAAC,CACJ,CAAC,EACC,CAAC,cAEVH,KAAA,YAASE,SAAS,CAAC,gBAAgB,CAAAC,QAAA,eACjCL,IAAA,OAAAK,QAAA,CAAI,qHAAoB,CAAI,CAAC,cAC7BH,KAAA,MAAAG,QAAA,EAAG,yhBACuB,cAAAL,IAAA,SAAAK,QAAA,CAAM,GAAC,CAAM,CAAC,0CAAS,cAAAL,IAAA,SAAAK,QAAA,CAAM,WAAS,CAAM,CAAC,+tBAE1B,EAAG,CAAC,cACjDL,IAAA,QAAAK,QAAA,cACEH,KAAA,SAAAG,QAAA,scAWM,CAAC,CACJ,CAAC,cACNL,IAAA,MAAAK,QAAA,CAAG,6lBACiC,CAAG,CAAC,EACjC,CAAC,cAEVH,KAAA,YAASE,SAAS,CAAC,gBAAgB,CAAAC,QAAA,eACjCL,IAA
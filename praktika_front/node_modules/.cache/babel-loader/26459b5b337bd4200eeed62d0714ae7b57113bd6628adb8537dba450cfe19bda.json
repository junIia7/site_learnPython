,{children:\"\\u0438\\u0442\\u0435\\u0440\\u0438\\u0440\\u0443\\u0435\\u043C\\u044B\\u0439_\\u043E\\u0431\\u044A\\u0435\\u043A\\u0442\"}),\" - \\u043E\\u0431\\u044A\\u0435\\u043A\\u0442, \\u043F\\u043E \\u043A\\u043E\\u0442\\u043E\\u0440\\u043E\\u043C\\u0443 \\u043F\\u0440\\u043E\\u0438\\u0441\\u0445\\u043E\\u0434\\u0438\\u0442 \\u0438\\u0442\\u0435\\u0440\\u0430\\u0446\\u0438\\u044F (\\u043D\\u0430\\u043F\\u0440\\u0438\\u043C\\u0435\\u0440, \\u0441\\u043F\\u0438\\u0441\\u043E\\u043A, \\u0434\\u0438\\u0430\\u043F\\u0430\\u0437\\u043E\\u043D).\"]}),/*#__PURE__*/_jsxs(\"li\",{children:[/*#__PURE__*/_jsx(\"code\",{children:\"\\u0443\\u0441\\u043B\\u043E\\u0432\\u0438\\u0435\"}),\" (\\u043D\\u0435\\u043E\\u0431\\u044F\\u0437\\u0430\\u0442\\u0435\\u043B\\u044C\\u043D\\u043E) - \\u0443\\u0441\\u043B\\u043E\\u0432\\u0438\\u0435, \\u043A\\u043E\\u0442\\u043E\\u0440\\u043E\\u0435 \\u0444\\u0438\\u043B\\u044C\\u0442\\u0440\\u0443\\u0435\\u0442 \\u044D\\u043B\\u0435\\u043C\\u0435\\u043D\\u0442\\u044B.\"]})]}),/*#__PURE__*/_jsx(\"pre\",{children:/*#__PURE__*/_jsxs(\"code\",{children:[\"numbers = [1, 2, 3, 4, 5]\\n\",\"squared_numbers = [x ** 2 for x in numbers]\\n\",\"print(squared_numbers)  # \\u0412\\u044B\\u0432\\u043E\\u0434: [1, 4, 9, 16, 25]\\n\\n\",\"even_numbers = [x for x in numbers if x % 2 == 0]\\n\",\"print(even_numbers)  # \\u0412\\u044B\\u0432\\u043E\\u0434: [2, 4]\\n\"]})}),/*#__PURE__*/_jsx(\"p\",{children:\"\\u0413\\u0435\\u043D\\u0435\\u0440\\u0430\\u0442\\u043E\\u0440\\u044B \\u0441\\u043F\\u0438\\u0441\\u043A\\u043E\\u0432 \\u044F\\u0432\\u043B\\u044F\\u044E\\u0442\\u0441\\u044F \\u044D\\u0444\\u0444\\u0435\\u043A\\u0442\\u0438\\u0432\\u043D\\u044B\\u043C \\u0438 \\u0447\\u0438\\u0442\\u0430\\u0435\\u043C\\u044B\\u043C \\u0441\\u043F\\u043E\\u0441\\u043E\\u0431\\u043E\\u043C \\u0441\\u043E\\u0437\\u0434\\u0430\\u043D\\u0438\\u044F \\u043D\\u043E\\u0432\\u044B\\u0445 \\u0441\\u043F\\u0438\\u0441\\u043A\\u043E\\u0432 \\u043D\\u0430 \\u043E\\u0441\\u043D\\u043E\\u0432\\u0435 \\u0441\\u0443\\u0449\\u0435\\u0441\\u0442\\u0432\\u0443\\u044E\\u0449\\u0438\\u0445 \\u0434\\u0430\\u043D\\u043D\\u044B\\u0445.\"})]}),/*#__PURE__*/_jsxs(\"section\",{className:\"lesson-section\",children:[/*#__PURE__*/_jsx(\"h2\",{children:\"\\u0417\\u0430\\u043A\\u043B\\u044E\\u0447\\u0435\\u043D\\u0438\\u0435\"}),/*#__PURE__*/_jsx(\"p\",{children:\"\\u0412 \\u044D\\u0442\\u043E\\u043C \\u0443\\u0440\\u043E\\u043A\\u0435 \\u043C\\u044B \\u0438\\u0437\\u0443\\u0447\\u0438\\u043B\\u0438 \\u043E\\u0441\\u043D\\u043E\\u0432\\u043D\\u044B\\u0435 \\u043A\\u043E\\u043D\\u0446\\u0435\\u043F\\u0446\\u0438\\u0438 \\u0438 \\u043E\\u043F\\u0435\\u0440\\u0430\\u0446\\u0438\\u0438, \\u0441\\u0432\\u044F\\u0437\\u0430\\u043D\\u043D\\u044B\\u0435 \\u0441\\u043E \\u0441\\u043F\\u0438\\u0441\\u043A\\u0430\\u043C\\u0438 \\u0438 \\u043A\\u043E\\u0440\\u0442\\u0435\\u0436\\u0430\\u043C\\u0438 \\u0432 Python. \\u0421\\u043F\\u0438\\u0441\\u043A\\u0438 \\u0438 \\u043A\\u043E\\u0440\\u0442\\u0435\\u0436\\u0438 \\u0438\\u0433\\u0440\\u0430\\u044E\\u0442 \\u0432\\u0430\\u0436\\u043D\\u0443\\u044E \\u0440\\u043E\\u043B\\u044C \\u0432 \\u043F\\u0440\\u043E\\u0433\\u0440\\u0430\\u043C\\u043C\\u0438\\u0440\\u043E\\u0432\\u0430\\u043D\\u0438\\u0438, \\u043E\\u0431\\u0435\\u0441\\u043F\\u0435\\u0447\\u0438\\u0432\\u0430\\u044F \\u0443\\u0434\\u043E\\u0431\\u0441\\u0442\\u0432\\u043E \\u0440\\u0430\\u0431\\u043E\\u0442\\u044B \\u0441 \\u0434\\u0430\\u043D\\u043D\\u044B\\u043C\\u0438 \\u0438 \\u043F\\u043E\\u0432\\u044B\\u0448\\u0430\\u044F \\u044D\\u0444\\u0444\\u0435\\u043A\\u0442\\u0438\\u0432\\u043D\\u043E\\u0441\\u0442\\u044C \\u0440\\u0430\\u0437\\u0440\\u0430\\u0431\\u043E\\u0442\\u043A\\u0438. \\u041F\\u0440\\u0430\\u043A\\u0442\\u0438\\u043A\\u0443\\u0439\\u0442\\u0435\\u0441\\u044C \\u0438 \\u044D\\u043A\\u0441\\u043F\\u0435\\u0440\\u0438\\u043C\\u0435\\u043D\\u0442\\u0438\\u0440\\u0443\\u0439\\u0442\\u0435 \\u0441 \\u0440\\u0430\\u0437\\u043B\\u0438\\u0447\\u043D\\u044B\\u043C\\u0438 \\u043E\\u043F\\u0435\\u0440\\u0430\\u0446\\u0438\\u044F\\u043C\\u0438 \\u0438 \\u043C\\u0435\\u0442\\u043E\\u0434\\u0430\\u043C\\u0438 \\u0434\\u043B\\u044F \\u043B\\u0443\\u0447\\u0448\\u0435\\u0433\\u043E \\u0443\\u0441\\u0432\\u043E\\u0435\\u043D\\u0438\\u044F \\u043C\\u0430\\u0442\\u0435\\u0440\\u0438\\u0430\\u043B\\u0430.\"})]}),/*#__PURE__*/_jsx(ScrollToTopButton,{})]});};export default ListsAndTuples;","map":{"version":3,"names":["React","ScrollToTopButton","jsx","_jsx","jsxs","_jsxs","ListsAndTuples","className","children"],"sources":["C:/Users/kasat/Desktop/praktika_front/src/pages/ListsAndTuples.js"],"sourcesContent":["import React from 'react';\r\nimport ScrollToTopButton from '../components/ScrollTopButton';\r\n\r\nconst ListsAndTuples = () => {\r\n  return (\r\n    <div className=\"lesson-container\">\r\n      <header className=\"lesson-header\">\r\n        <h1>Списки и кортежи</h1>\r\n      </header>\r\n\r\n      <section className=\"lesson-section\">\r\n        <h2>Определение и различия</h2>\r\n        <p>Список в Python представляет собой упорядоченную изменяемую коллекцию элементов, которая может содержать объекты \r\n          любых типов, заключённые в квадратные скобки. Например:</p>\r\n        <pre>\r\n          <code>\r\n            {`my_list = [1, 2, 3, 'apple', 'banana']\\n`}\r\n            {`print(my_list)  # Вывод: [1, 2, 3, 'apple', 'banana']\\n`}\r\n          </code>\r\n        </pre>\r\n        <p>Кортеж в Python представляет собой упорядоченную неизменяемую коллекцию элементов, которая может содержать \r\n          объекты любых типов, заключённые в круглые скобки. Например:</p>\r\n        <pre>\r\n          <code>\r\n            {`my_tuple = (1, 2, 3, 'apple', 'banana')\\n`}\r\n            {`print(my_tuple)  # Вывод: (1, 2, 3, 'apple', 'banana')\\n`}\r\n          </code>\r\n        </pre>\r\n        <p>Основное различие между списками и кортежами заключается в том, что списки можно изменять (добавлять, удалять элементы)\r\n          , а кортежи являются неизменяемыми после создания. Это делает кортежи более эффективными в использовании, если \r\n          требуется неизменяемая коллекция данных.</p>\r\n      </section>\r\n\r\n      <section className=\"lesson-section\">\r\n        <h2>Операции над списками и кортежами</h2>\r\n        <p>Списки и кортежи поддерживают множество общих операций, таких как доступ к элементам по индексу, срезы (slices), \r\n          конкатенация и повторение. Например:</p>\r\n        <pre>\r\n          <code>\r\n            {`my_list = [1, 2, 3]\\n`}\r\n            {`my_tuple = (4, 5, 6)\\n\\n`}\r\n\r\n            {`print(my_list[0])  # Вывод: 1\\n`}\r\n            {`print(my_tuple[1:])  # Вывод: (5, 6)\\n\\n`}\r\n\r\n            {`combined = my_list + list(my_tuple)\\n`}\r\n            {`print(combined)  # Вывод: [1, 2, 3, 4, 5, 6]\\n\\n`}\r\n\r\n            {`repeated = my_tuple * 2\\n`}\r\n            {`print(repeated)  # Вывод: (4, 5, 6, 4, 5, 6)\\n`}\r\n          </code>\r\n        </pre>\r\n        <p>Операции над списками и кортежами позволяют эффективно работать с элементами коллекций в Python.</p>\r\n      </section>\r\n\r\n      <section className=\"lesson-section\">\r\n        <h2>Методы списков</h2>\r\n        <p>Списки в Python имеют ряд встроенных методов для удобного управления элементами:</p>\r\n        <ul>\r\n          <li><code>append(element)</code> - добавляет элемент в конец списка.</li>\r\n          <li><code>extend(iterable)</code> - расширяет список, добавляя элементы из итерируемого объекта.</li>\r\n          <li><code>insert(index, element)</code> - вставляет элемент в указанную позицию.</li>\r\n          <li><code>remove(element)</code> - удаляет первое вхождение указанного элемента из списка.</li>\r\n          <li><code>pop([index])</code> - удаляет и возвращает элемент по индексу (по умолчанию последний).</li>\r\n        </ul>\r\n          <p>Эти методы делают списки мощным инструментом для управления коллекциями данных.</p>\r\n      </section>\r\n\r\n      <section className=\"lesson-section\">\r\n        <h2>Генераторы списков</h2>\r\n        <p>Генераторы списков (list comprehensions) представляют собой компактный способ создания списков в Python с \r\n          использованием выражений. Они позволяют быстро и чисто создавать списки на основе итерации и условий. Например:</p>\r\n        <p>Основной синтаксис генератора списка выглядит следующим образом:</p>\r\n        <pre>\r\n          <code>\r\n            {`[выражение for элемент in итерируемый_объект if условие]\\n`}\r\n          </code>\r\n        </pre>\r\n        <p>Здесь:</p>\r\n        <ul>\r\n          <li><code>выражение</code> - выражение, которое применяется к каждому элементу.</li>\r\n          <li><code>элемент</code> - текущий элемент из <code>итерируемого_объекта</code>.</li>\r\n          <li><code>итерируемый_объект</code> - объект, по которому происходит итерация (например, список, диапазон).</li>\r\n          <li><code>условие</code> (необязательно) - условие, которое фильтрует элементы.</li>\r\n        </ul>\r\n        <pre>\r\n          <code>\r\n            {`numbers = [1, 2, 3, 4, 5]\\n`}\r\n            {`squared_numbers = [x ** 2 for x in numbers]\\n`}\r\n            {`print(squared_numbers)  # Вывод: [1, 4, 9, 16, 25]\\n\\n`}\r\n\r\n            {`even_numbers = [x for x in numbers if x % 2 == 0]\\n`}\r\n            {`print(even_numbers)  # Вывод: [2, 4]\\n`}\r\n          </code>\r\n        </pre>\r\n        <p>Генераторы списков являются эффективным и читаемым способом создания новых списков на основе существующих данных.</p>\r\n      </section>\r\n\r\n      <section className=\"lesson-section\">\r\n        <h2>Заключение</h2>\r\n        <p>В этом уроке мы изучили основные концепции и операции, связанные со списками и кортежами в Python.\r\n          Списки и кортежи играют важную роль в программировании, обеспечивая удобство работы с данными и повышая эффективность \r\n          разработки. Практикуйтесь и экспериментируйте с различными операциями и методами для лучшего усвоения материала.</p>\r\n      </section>\r\n      <ScrollToTopButton /> \r\n    </div>\r\n  );\r\n};\r\n\r\nexport default ListsAndTuples;\r\n"],"mappings":"AAAA,MAAO,CAAAA,KAAK,KAAM,OAAO,CACzB,MAAO,CAAAC,iBAAiB,KAAM,+BAA+B,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAE9D,KAAM,CAAAC,cAAc,CAAGA,CAAA,GAAM,CAC3B,mBACED,KAAA,QAAKE,SAAS,CAAC,kBAAkB,CAAAC,QAAA,eAC/BL,IAAA,WAAQI,SAAS,CAAC,eAAe,CAAAC,QAAA,cAC/BL,IAAA,OAAAK,QAAA,CAAI,wFAAgB,CAAI,CAAC,CACnB,CAAC,cAETH,KAAA,YAASE,SAAS,CAAC,gBAAgB,CAAAC,QAAA,eACjCL,IAAA,OAAAK,QAAA,CAAI,4HAAsB,CAAI,CAAC,cAC/BL,IAAA,MAAAK,QAAA,CAAG,i2BACsD,CAAG,CAAC,cAC7DL,IAAA,QAAAK,QAAA,cACEH,KAAA,SAAAG,QAAA,iIAGM,CAAC,CACJ,CAAC,cACNL,IAAA,MAAAK,QAAA,CAAG,21BAC2D,CAAG,CAAC,cAClEL,IAAA,QAAAK,QAAA,cACEH,KAAA,SAAAG,QAAA,mIAGM,CAAC,CACJ,CAAC,cACNL,IAAA,MAAAK,QAAA,CAAG,04CAEuC,CAAG,CAAC,EACvC,CAAC,cAEVH,KAAA,YAASE,SAAS,CAAC,gBAAgB,CAAAC,QAAA,eACjCL,IAAA,OAAAK,QAAA,CAAI,oLAAiC,CAAI,CAAC,cAC1CL,IAAA,MAAAK,QAAA,CAAG,guBACmC,CAAG,CAAC,cAC1CL,IAAA,QAAAK,QAAA,cACEH,KAAA,SAAAG,QAAA,+YAYM,CAAC,CACJ,CAAC,cACNL,IAAA,MAAAK,QAAA,CAAG,meAAgG,CAAG,CAAC,EAChG,CAAC,cAEVH,KAAA,YAASE,SAAS,CAAC,gBAAgB,CAAAC,QAAA,eACjCL,IAAA,OAAAK,QAAA,CAAI,iFAAc,CAAI,CAAC,cACvBL,IAAA,MAAAK,QAAA,CAAG,6YAAgF,CAAG,CAAC,cACvFH,KAAA,OAAAG,QAAA,eACEH,KAAA,OAAAG,QAAA,eAAIL,IAAA,SAAAK,QAAA,CAAM,iBAAe,CAAM,CAAC,mLAAoC,EAAI,CAAC,cACzEH,KAAA,OAAAG,QAAA,eAAIL,IAAA,SAAAK,QAAA,CAAM,kBAAgB,CAAM,CAAC,sUAA+D,EAAI,CAAC,cACrGH,KAAA,OAAAG,QAAA,eAAIL,IAAA,SAAAK,QAAA,CAAM,wBAAsB,CAAM,CAAC,iNAAyC,EAAI,CAAC,cACrFH,KAAA,OAAAG,QAAA,eAAIL,IAAA,SAAAK,QAAA,CAAM,iBAAe,CAAM,CAAC,6SAA0D,EAAI,CAAC,cAC/FH,KAAA,OAAAG,QAAA,eAAIL,IAAA,SAAAK,QAAA,CAAM,cAAY,CAAM,CAAC,qVAAoE,EAAI,CAAC,EACpG,CAAC,cACHL,IAAA,MAAAK,QAAA,CAAG,0aAA+E,CAAG,CAAC,EACjF,CAAC,cAEVH,KAAA,YAASE,SAAS,CAAC,gBAAgB,CAAAC,QAAA,eACjCL,IAAA,OAAAK,QAAA,CAAI,yGAAkB,CAAI,CAAC,cAC3BL,IAAA,MAAAK,QAAA,CAAG,ggCAC8G,CAAG,CAAC,cACrHL,IAAA,MAAAK,QAAA,CAAG,+VAAgE,CAAG,CAAC,cACvEL,IAAA,QAAAK,QA